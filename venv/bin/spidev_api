#!/Users/bright/PycharmProjects/TDK24/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'spidev-api==0.0.1','console_scripts','spidev_api'
__requires__ = 'spidev-api==0.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('spidev-api==0.0.1', 'console_scripts', 'spidev_api')()
    )
